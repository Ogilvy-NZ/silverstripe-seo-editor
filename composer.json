{
    "name": "littlegiant/silverstripe-seo-editor",
    "type": "silverstripe-vendormodule",
    "description": "SEO Editor Administration for SilverStripe",
    "keywords": ["silverstripe", "seo", "editing", "metatitle", "metadescription"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stevie Mayhew",
            "email": "stevie.mayhew@littlegiant.co.nz"
        }
    ],
    "require": {
        "php": "^8.1",
        "silverstripe/cms": "^5",
        "kinglozzer/metatitle": "^3"
    },
    "extra": {
        "expose": [
            "client/css",
            "client/javascript"
        ]
    },
    "autoload": {
        "psr-4": {
            "LittleGiant\\SEOEditor\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
